<mvc:View controllerName="com.lab2dev.firstapp.controller.Home"
    xmlns:mvc="sap.ui.core.mvc" displayBlock="true"
    xmlns="sap.m">
    <Page id="page" title="{i18n>title}">
        <content>
            <List headerText="Produtos" headerLevel="H2" items="{products>/}" >
                <StandardListItem 
                    type="Active" 
                    press="onPress" 
                    title="{products>title}" 
                    counter="{products>counter}" />
            </List>
        </content>
    </Page>
</mvc:View>
